var mainWin = Ti.UI.createWindow({
	title: "Project Week 2",
	modal: true,
	backgroundColor: "#595BC2"
	
});

var navWin = Ti.UI.iOS.createNavigationWindow({
	window: mainWin
});

var galleryView = Ti.UI.createView({
	backgroundColor: "#3605E8",
	height: 50,
	top: 10
	
});

var dataView =Ti.UI.createView({
	backgroundColor:"#E80505",
	height: 50,
	top: galleryView.top + 10
	
});

var galleryLabel = Ti.UI.createLabel({
	text: "Gallery",
	color: "#fff",
	font: {fontSize: 20, fontFamily: "Times New Roman"},
	textAlign: "center"
	
});

var dataLabel = Ti.UI.createLabel({
	text: "Data",
	color: "#fff",
	font: {fontSize: 20, fontFamily: "Times New Roman"},
	textAlign: "center"
	
});

dataView.add(dataLabel);
galleryView.add(galleryLabel);
mainWin.add(galleryView, dataView);
navWin.open();